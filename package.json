{
  "name": "syncfusion-theme-studio",
  "version": "0.1.0",
  "description": "A high-performance, fully customizable React admin portal template with visual theme editor",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/syncfusion-theme-studio.git"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "tailwindcss",
    "syncfusion",
    "admin-portal",
    "theme-editor",
    "dashboard"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && node scripts/strip-google-fonts.cjs && node scripts/add-prefetch-hints.js",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "api:generate": "orval --config orval.config.ts",
    "typecheck": "tsc --noEmit",
    "lighthouse:prod": "node scripts/run-lighthouse.js http://localhost:4445 --output html --output-path ./reports/lighthouse-prod.html --budget-path=./lighthouse-budget.json --preset=desktop --chrome-flags=\"--headless=new\"",
    "lighthouse:prod:ci": "node scripts/run-lighthouse.js http://localhost:4445 --output json --output-path ./reports/lighthouse-prod.json --preset=desktop --chrome-flags=\"--headless=new --no-sandbox\" --budget-path=./lighthouse-budget.json",
    "lighthouse:prod:open": "start ./reports/lighthouse-prod.html",
    "lighthouse:prod:assert": "node scripts/lighthouse-assert.js reports/lighthouse-prod.json",
    "analyze": "vite build --mode analyze",
    "figma:extract": "tsx --env-file=.env scripts/figma/extract.ts",
    "figma:discover": "tsx --env-file=.env scripts/figma/discover.ts",
    "figma:generate": "tsx scripts/figma/generate.ts",
    "figma:sync": "tsx --env-file=.env scripts/figma/extract.ts && tsx scripts/figma/generate.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@syncfusion/ej2-react-buttons": "^32.2.3",
    "@syncfusion/ej2-react-calendars": "^32.2.3",
    "@syncfusion/ej2-react-dropdowns": "^32.2.3",
    "@syncfusion/ej2-react-grids": "^32.2.3",
    "@syncfusion/ej2-react-inputs": "^32.2.3",
    "@syncfusion/ej2-react-layouts": "^32.2.3",
    "@syncfusion/ej2-react-navigations": "^32.2.3",
    "@syncfusion/ej2-react-notifications": "^32.2.3",
    "@syncfusion/ej2-react-popups": "^32.2.3",
    "@tanstack/react-query": "^5.51.0",
    "axios": "^1.6.0",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.42.0",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^15.0.0",
    "jsdom": "^28.0.0",
    "jsonc-eslint-parser": "^2.4.2",
    "lighthouse": "^12.0.0",
    "orval": "^8.2.0",
    "postcss": "^8.4.0",
    "postcss-import": "^16.1.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.0",
    "terser": "^5.46.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.3.1",
    "vite-bundle-analyzer": "^1.3.6",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "braces": "^3.0.3",
    "micromatch": "^4.0.8"
  }
}
